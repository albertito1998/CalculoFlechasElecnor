<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="64dp"
        android:layout_height="29dp"
        android:layout_x="8dp"
        android:layout_y="6dp"
        android:src="@drawable/logotess" />

    <TextView
        android:id="@+id/Lbempresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_x="90dp"
        android:layout_y="14dp"
        android:text="ELECNOR PORYECTOS Y SEFVICIOS" />
    <TextView android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Introduzca los siguientes datos:" android:layout_x="14dp" android:layout_y="73dp"/>
    <TextView android:textStyle="bold" android:id="@+id/LbTitulo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="FLECHAR 1 VANO" android:scrollHorizontally="false" android:layout_x="94dp" android:layout_y="46dp"/>
    <TextView android:id="@+id/TextView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Flecha teórica:" android:layout_x="12dp" android:layout_y="249dp"/>
    <TextView android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Longitud del vano:" android:layout_x="12dp" android:layout_y="203dp"/>
    <TextView android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Ángulo en grapa:" android:layout_x="12dp" android:layout_y="155dp"/>
    <TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Distancia taquimetro a fase:" android:layout_x="12dp" android:layout_y="107dp"/>
    <EditText android:textSize="10dp" android:id="@+id/txtDistancia" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="209dp" android:layout_y="101dp" android:inputType="numberDecimal"/>
    <EditText android:textSize="10dp" android:id="@+id/txtanggrapa" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="209dp" android:layout_y="149dp" android:inputType="numberDecimal"/>

    <EditText
        android:id="@+id/txtlongvano"
        android:layout_width="72dp"
        android:layout_height="35dp"
        android:layout_x="209dp"
        android:layout_y="197dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textSize="10dp"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
    <EditText android:textSize="10dp" android:id="@+id/txtflechteor" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="209dp" android:layout_y="243dp" android:inputType="numberDecimal"/>
    <Button android:id="@+id/cmdComprobar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Comprobar" android:layout_x="109dp" android:layout_y="308dp" style="?android:attr/buttonStyleSmall"/>
    <TextView android:id="@+id/txtanguloflechar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Ángulo para flechar:" android:layout_x="20dp" android:layout_y="377dp"/>
    <TextView android:id="@+id/txterror1" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="297dp" android:layout_y="107dp"/>
    <TextView android:id="@+id/txterror2" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="297dp" android:layout_y="153dp"/>
    <TextView android:id="@+id/txterror3" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="297dp" android:layout_y="202dp"/>
    <TextView android:id="@+id/txterror4" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="297dp" android:layout_y="247dp"/>
    <TextView android:id="@+id/txtMensajeError" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="(*) Falta" android:layout_x="252dp" android:layout_y="293dp"/>
    <Button android:id="@+id/cmdVolver" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Volver" android:layout_x="248dp" android:layout_y="379dp" style="?android:attr/buttonStyleSmall"/>
    <Button android:textSize="8dp" android:id="@+id/cmdVaciar" android:layout_width="wrap_content" android:layout_height="32dp" android:text="Vaciar" android:layout_x="258dp" android:layout_y="66dp" style="?android:attr/buttonStyleSmall"/>
    <ImageButton android:id="@+id/cmdAyuda" android:layout_width="24dp" android:layout_height="21dp" android:src="@drawable/interrogacion" android:layout_x="58dp" android:layout_y="44dp"/>
</AbsoluteLayout>
