    <TextView android:id="@+id/txtFlechaReal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Flecha Real: " android:layout_x="20dp" android:layout_y="376dp"/>
    <TextView android:id="@+id/txtErr1" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="293dp" android:layout_y="110dp"/>
    <TextView android:id="@+id/txtErr2" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="293dp" android:layout_y="146dp"/>
    <TextView android:id="@+id/txtErr3" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="293dp" android:layout_y="186dp"/>
    <TextView android:id="@+id/txtErr4" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="293dp" android:layout_y="225dp"/>
    <TextView android:id="@+id/txtErr5" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="293dp" android:layout_y="262dp"/>
