<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_x="8dp"
        android:layout_y="6dp"
        android:src="@drawable/logotess"
        android:contentDescription="@string/logo_desc" />

    <TextView
        android:id="@+id/Lbempresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_x="90dp"
        android:layout_y="14dp"
        android:text="@string/empresa"
        android:contentDescription="@string/empresa" />

    <TextView
        android:id="@+id/LbTitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="64dp"
        android:layout_y="46dp"
        android:scrollHorizontally="false"
        android:text="@string/titulo"
        android:textStyle="bold"
        android:contentDescription="@string/titulo" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="14dp"
        android:layout_y="73dp"
        android:text="@string/introduccion"
        android:contentDescription="@string/introduccion" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="107dp"
        android:text="@string/distancia_taquimetro"
        android:contentDescription="@string/distancia_taquimetro_cd" />

    <EditText
        android:id="@+id/txtDistancia"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_x="203dp"
        android:layout_y="101dp"
        android:inputType="numberDecimal"
        android:textSize="14sp"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/distancia_taquimetro_cd"
        android:autofillHints="distance" />

    <TextView
        android:id="@+id/TextView05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="271dp"
        android:text="@string/flecha_teorica"
        android:contentDescription="@string/flecha_teorica_cd" />

    <EditText
        android:id="@+id/TxtFlechaTeorica"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_x="203dp"
        android:layout_y="265dp"
        android:inputType="numberDecimal"
        android:textSize="14sp"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/flecha_teorica_cd"
        android:autofillHints="theoretical_arrow"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="189dp"
        android:text="@string/angulo_cable"
        android:contentDescription="@string/angulo_cable_cd" />

    <EditText
        android:id="@+id/TxtAnguloCable"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_x="203dp"
        android:layout_y="183dp"
        android:inputType="numberDecimal"
        android:textSize="14sp"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/angulo_cable_cd"
        android:autofillHints="cable_angle"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/TextView04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="230dp"
        android:text="@string/longitud_vano"
        android:contentDescription="@string/longitud_vano_cd" />

    <EditText
        android:id="@+id/TxtLongitudVano"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_x="203dp"
        android:layout_y="224dp"
        android:inputType="numberDecimal"
        android:textSize="14sp"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/longitud_vano_cd"
        android:autofillHints="span_length"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="148dp"
        android:text="@string/angulo_grapa"
        android:contentDescription="@string/angulo_grapa_cd" />

    <EditText
        android:id="@+id/TxtAnguloGrapa"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_x="203dp"
        android:layout_y="142dp"
        android:inputType="numberDecimal"
        android:textSize="14sp"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/angulo_grapa_cd"
        android:autofillHints="grip_angle"
        tools:ignore="LabelFor" />

    <Button
        android:id="@+id/cmdComprobar"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="108dp"
        android:layout_y="319dp"
        android:text="@string/comprobar"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/comprobar_cd" />

    <TextView
        android:id="@+id/txtFlechaReal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="20dp"
        android:layout_y="376dp"
        android:text="@string/flecha_real" />
    <TextView
        android:id="@+id/unidadesDistancia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="293dp"
        android:layout_y="110dp"
        android:text="m"   />

    <TextView
        android:id="@+id/txtErr1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="110dp"
        android:text="@string/error_msg"
        android:visibility="invisible" />
    <TextView
        android:id="@+id/unidadesAngulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="293dp"
        android:layout_y="146dp"
        android:text="ยบ"   />
    <TextView
        android:id="@+id/txtErr2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="146dp"
        android:text="@string/error_msg"
        android:visibility="invisible" />
    <TextView
        android:id="@+id/unidadesAngulo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="293dp"
        android:layout_y="186dp"
        android:text="ยบ"   />
    <TextView
        android:id="@+id/txtErr3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="186dp"
        android:text="@string/error_msg"
        android:visibility="invisible" />
    <TextView
        android:id="@+id/unidadesDistancia1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="293dp"
        android:layout_y="225dp"
        android:text="m"   />
    <TextView
        android:id="@+id/txtErr4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="225dp"
        android:text="@string/error_msg"
        android:visibility="invisible" />
    <TextView
        android:id="@+id/unidadesDistancia2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="293dp"
        android:layout_y="262dp"
        android:text="m"   />
    <TextView
        android:id="@+id/txtErr5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="262dp"
        android:text="@string/error_msg"
        android:visibility="invisible" />

    <Button
        android:id="@+id/cmdVolver"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="248dp"
        android:layout_y="450dp"
        android:text="@string/volver"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/volver_cd" />

    <TextView
        android:id="@+id/TxtCableBajo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="20dp"
        android:layout_y="404dp"
        android:text="@string/cable_bajo" />
    <TextView
        android:id="@+id/unidadesDistancia3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="293dp"
        android:layout_y="376dp"
        android:text="m"   />
    <TextView
        android:id="@+id/txtMensajeError"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="376dp"
        android:text="@string/mensaje_error"
        android:visibility="invisible" />

    <Button
        android:id="@+id/cmdVaciar"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_x="258dp"
        android:layout_y="50dp"
        android:text="@string/vaciar"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/vaciar_cd" />

    <ImageButton
        android:id="@+id/cmdAyuda"
        android:layout_width="24dp"
        android:layout_height="21dp"
        android:layout_x="32dp"
        android:layout_y="46dp"
        android:src="@drawable/interrogacion"
        android:contentDescription="@string/ayuda_desc" />

    <TextView
        android:id="@+id/tvSelectedUnits"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/selected_units"
        android:layout_x="50dp"
        android:layout_y="450dp"/>
</AbsoluteLayout>