    <EditText android:textSize="10dp" android:id="@+id/TxtAnguloGrapa" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="203dp" android:layout_y="142dp" android:inputType="numberDecimal">
        <requestFocus/>
    </EditText>
    <EditText android:textSize="10dp" android:id="@+id/TxtAnguloCable" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="203dp" android:layout_y="183dp" android:inputType="numberDecimal"/>
