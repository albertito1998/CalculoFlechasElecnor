<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_x="8dp"
        android:layout_y="6dp"
        android:src="@drawable/logotess" />

    <TextView
        android:id="@+id/Lbempresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_x="90dp"
        android:layout_y="14dp"
        android:text="ELECNOR PORYECTOS Y SERVICIOS" />
    <TextView android:textStyle="bold" android:id="@+id/LbTitulo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="COMPROBAR FLECHA 1 VANO" android:scrollHorizontally="false" android:layout_x="64dp" android:layout_y="46dp"/>
    <TextView android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Introduzca los siguientes datos:" android:layout_x="14dp" android:layout_y="73dp"/>
    <TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Distancia taquimetro a fase:" android:layout_x="12dp" android:layout_y="107dp"/>
    <EditText android:textSize="10dp" android:id="@+id/txtDistancia" android:layout_width="72dp" android:layout_height="35dp" android:layout_x="203dp" android:layout_y="101dp" android:inputType="numberDecimal"/>
    <TextView android:id="@+id/TextView05" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Flecha teórica:" android:layout_x="12dp" android:layout_y="271dp"/>
    <TextView android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Ángulo en cable:" android:layout_x="12dp" android:layout_y="189dp"/>
