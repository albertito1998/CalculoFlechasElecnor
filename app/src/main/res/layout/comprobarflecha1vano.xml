    <TextView android:id="@+id/TxtCableBajo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cable Bajo:" android:layout_x="20dp" android:layout_y="404dp"/>
    <TextView android:id="@+id/txtMensajeError" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="(*) Falta" android:layout_x="252dp" android:layout_y="315dp"/>
    <Button android:textSize="8dp" android:id="@+id/cmdVaciar" android:layout_width="wrap_content" android:layout_height="32dp" android:text="Vaciar" android:layout_x="258dp" android:layout_y="72dp" style="?android:attr/buttonStyleSmall"/>
    <ImageButton android:id="@+id/cmdAyuda" android:layout_width="24dp" android:layout_height="21dp" android:src="@drawable/interrogacion" android:layout_x="32dp" android:layout_y="46dp"/>
</AbsoluteLayout>
