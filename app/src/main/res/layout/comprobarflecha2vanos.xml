<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="64dp"
        android:layout_height="27dp"
        android:layout_x="8dp"
        android:layout_y="6dp"
        android:src="@drawable/logotess" />

    <TextView
        android:id="@+id/Lbempresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_x="90dp"
        android:layout_y="14dp"
        android:text="ELECNOR PROYECTOS Y SERVICIOS" />
    <TextView android:textStyle="bold" android:id="@+id/LbTitulo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="COMPROBAR FLECHA EN 2 VANOS" android:scrollHorizontally="false" android:layout_x="64dp" android:layout_y="42dp"/>
    <TextView android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Introduzca los siguientes datos:" android:layout_x="14dp" android:layout_y="64dp"/>
    <TextView android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Ángulo en grapa 1:" android:layout_x="13dp" android:layout_y="94dp"/>
    <TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Ángulo en grapa 2:" android:layout_x="13dp" android:layout_y="135dp"/>
    <TextView android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Ángulo en cable del vano 2:" android:layout_x="13dp" android:layout_y="176dp"/>
    <TextView android:id="@+id/TextView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Longitud del vano 1:" android:layout_x="13dp" android:layout_y="217dp"/>
    <TextView android:id="@+id/TextView05" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Longitud del vano 2:" android:layout_x="13dp" android:layout_y="258dp"/>
    <EditText android:textSize="10dp" android:id="@+id/txtanggrapa1" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="210dp" android:layout_y="88dp" android:inputType="numberDecimal"/>
    <EditText android:textSize="10dp" android:id="@+id/txtanggrapa2" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="210dp" android:layout_y="129dp" android:inputType="numberDecimal"/>
    <EditText android:textSize="10dp" android:id="@+id/txtangcablevano2" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="210dp" android:layout_y="170dp" android:inputType="numberDecimal"/>
    <EditText android:textSize="10dp" android:id="@+id/txtlongvano1" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="210dp" android:layout_y="211dp" android:inputType="numberDecimal"/>
    <EditText android:textSize="10dp" android:id="@+id/txtlongvano2" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="210dp" android:layout_y="252dp" android:inputType="numberDecimal"/>
    <TextView android:id="@+id/TxtErr1" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="298dp" android:layout_y="91dp"/>
    <TextView android:id="@+id/TxtErr2" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="298dp" android:layout_y="132dp"/>
    <TextView android:id="@+id/TxtErr3" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="298dp" android:layout_y="173dp"/>
    <TextView android:id="@+id/TxtErr4" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="298dp" android:layout_y="214dp"/>
    <TextView android:id="@+id/TxtErr5" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="298dp" android:layout_y="255dp"/>
    <TextView android:id="@+id/TxtErr6" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="*" android:layout_x="298dp" android:layout_y="296dp"/>
    <TextView android:id="@+id/TextView06" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="- Flecha teórica del vano 2:" android:layout_x="13dp" android:layout_y="299dp"/>
    <EditText android:textSize="10dp" android:id="@+id/TxtFlechavano2" android:layout_width="72dp" android:layout_height="35dp" android:ems="10" android:layout_x="210dp" android:layout_y="293dp" android:inputType="numberDecimal">
        <requestFocus/>
    </EditText>
    <Button android:id="@+id/cmdVolver" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Volver" android:layout_x="248dp" android:layout_y="379dp" style="?android:attr/buttonStyleSmall"/>
    <Button android:id="@+id/cmdComprobar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Comprobar" android:layout_x="107dp" android:layout_y="335dp" style="?android:attr/buttonStyleSmall"/>
    <TextView android:id="@+id/txtFlechaReal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Flecha Real: " android:layout_x="20dp" android:layout_y="382dp"/>
    <TextView android:id="@+id/TxtCableBajo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cable Bajo:" android:layout_x="20dp" android:layout_y="408dp"/>
    <TextView android:id="@+id/txtMensajeError" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="(*) Falta" android:layout_x="253dp" android:layout_y="349dp"/>
    <Button android:textSize="8dp" android:id="@+id/cmdVaciar" android:layout_width="wrap_content" android:layout_height="32dp" android:text="Vaciar" android:layout_x="268dp" android:layout_y="59dp" style="?android:attr/buttonStyleSmall"/>
    <ImageButton android:id="@+id/cmdAyuda" android:layout_width="24dp" android:layout_height="21dp" android:src="@drawable/interrogacion" android:layout_x="38dp" android:layout_y="39dp"/>
</AbsoluteLayout>
